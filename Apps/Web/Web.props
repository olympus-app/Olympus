		<ProjectReference Include="$(SolutionFolder)\Modules\Core\Frontend\Core.Frontend.csproj" />
	</ItemGroup>

	<!-- Usings -->
	<ItemGroup>
		<Using Include="Humanizer" />
		<Using Include="Olympus.Shared" />
		<Using Include="Olympus.Kernel" />
		<Using Include="Olympus.Core.Archend" />
		<Using Include="Olympus.Core.Frontend" />
		<Using Include="Olympus.Web" />
		<Using Include="Microsoft.Extensions.DependencyInjection" />
	</ItemGroup>

</Project>