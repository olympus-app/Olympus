<Project Sdk="Microsoft.NET.Sdk">

	<!-- Project Properties -->
	<PropertyGroup Label="ProjectProperties">
		<ProjectType>AspireService</ProjectType>
		<ProjectLayer>None</ProjectLayer>
		<IsAspireSharedProject>true</IsAspireSharedProject>
	</PropertyGroup>

	<!-- Framework Reference -->
	<ItemGroup Label="FrameworkReferences">
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<!-- Project References -->
	<ItemGroup Label="ProjectReferences">
		<ProjectReference Include="$(CoreProjectFile)" />
	</ItemGroup>

	<!-- Packages References -->
	<ItemGroup Label="PackagesReferences">
		<PackageReference Include="Microsoft.Extensions.Http.Resilience" />
		<PackageReference Include="Microsoft.Extensions.ServiceDiscovery" />
		<PackageReference Include="OpenTelemetry.Exporter.OpenTelemetryProtocol" />
		<PackageReference Include="OpenTelemetry.Extensions.Hosting" />
		<PackageReference Include="OpenTelemetry.Instrumentation.AspNetCore" />
		<PackageReference Include="OpenTelemetry.Instrumentation.EntityFrameworkCore" />
		<PackageReference Include="OpenTelemetry.Instrumentation.Http" />
		<PackageReference Include="OpenTelemetry.Instrumentation.Runtime" />
		<PackageReference Include="OpenTelemetry.Instrumentation.StackExchangeRedis" />
	</ItemGroup>

</Project>