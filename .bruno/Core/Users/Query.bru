meta {
  name: Query
  type: http
  seq: 1
}

get {
  url: {{BasePath}}/api/v1/core/users/query
  body: none
  auth: none
}

params:query {
  ~page: 1
  ~pageSize: 10
  ~filter: id={{UserId}}
  ~orderBy: id desc
}

headers {
  ~If-None-Match: {{UserQueryEtag}}
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("UserQueryEtag", etag);
}
