meta {
  name: Photo
  type: http
  seq: 7
}

post {
  url: {{BasePath}}/api/v1/core/users/photo?id={{UserId}}
  body: multipartForm
  auth: none
}

params:query {
  id: {{UserId}}
}

body:multipart-form {
  file: @file(C:\Sandbox\Icon.png)
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("PhotoEtag", etag);
}
