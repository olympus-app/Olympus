meta {
  name: Photo
  type: http
  seq: 7
}

post {
  url: {{BasePath}}/api/v1/core/users/:id/photo
  body: multipartForm
  auth: none
}

params:path {
  id: {{UserId}}
}

body:multipart-form {
  file: @file(C:\Sandbox\Icon.png)
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("PhotoEtag", etag);
}
