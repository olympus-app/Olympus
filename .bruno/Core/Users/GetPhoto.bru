meta {
  name: Photo
  type: http
  seq: 6
}

get {
  url: {{BasePath}}/api/v1/core/users/photo?id={{UserId}}&size=small
  body: none
  auth: none
}

params:query {
  id: {{UserId}}
  size: small
}

headers {
  ~If-None-Match: {{PhotoEtag}}
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("PhotoEtag", etag);
}
