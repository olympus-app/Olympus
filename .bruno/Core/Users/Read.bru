meta {
  name: Read
  type: http
  seq: 2
}

get {
  url: {{BasePath}}/api/v1/core/users/read?id={{UserId}}
  body: none
  auth: none
}

params:query {
  id: {{UserId}}
}

headers {
  ~If-None-Match: {{UserReadEtag}}
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("UserReadEtag", etag);
}
