meta {
  name: Delete
  type: http
  seq: 5
}

delete {
  url: {{BasePath}}/api/v1/core/users/:id?force=true
  body: none
  auth: none
}

params:query {
  force: true
}

params:path {
  id: {{TestId}}
}

headers {
  If-Match: {{TestEtag}}
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("TestEtag", etag);
}

settings {
  encodeUrl: true
  timeout: 0
}
