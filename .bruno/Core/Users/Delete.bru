meta {
  name: Delete
  type: http
  seq: 5
}

delete {
  url: {{BasePath}}/api/v1/core/users/delete?id={{UserTestId}}&force=true
  body: none
  auth: none
}

params:query {
  id: {{UserTestId}}
  force: true
}

headers {
  If-Match: {{UserTestEtag}}
}

script:post-response {
  const etag = res.getHeader("etag");
  if (etag) bru.setVar("UserTestEtag", etag);
}

settings {
  encodeUrl: true
  timeout: 0
}
