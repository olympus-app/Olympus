<Project>

	<!-- Compilation Properties -->
	<PropertyGroup Label="CompilationProperties">
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<RootNamespace>$(ProjectName)</RootNamespace>
		<OlympusImplicitUsings>enable</OlympusImplicitUsings>
		<OlympusImplicitReferences>enable</OlympusImplicitReferences>
		<SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
		<IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
	</PropertyGroup>

	<!-- Compilation Properties for Aspire Application Package -->
	<PropertyGroup Label="CompilationProperties" Condition="$(IsAspireApplicationPackage)">
		<IsAspireSharedProject>true</IsAspireSharedProject>
	</PropertyGroup>

	<!-- Compilation Properties for Aspire Application Host -->
	<PropertyGroup Label="CompilationProperties" Condition="$(IsAspireApplicationHost)">
		<OutputType>Exe</OutputType>
	</PropertyGroup>

	<!-- Compilation Properties for Web Application Host -->
	<PropertyGroup Label="CompilationProperties" Condition="$(IsWebApplication)">
		<CompressionEnabled>false</CompressionEnabled>
		<PublishTrimmed>false</PublishTrimmed>
	</PropertyGroup>

</Project>