<Project>

	<!-- Compilation Properties -->
	<PropertyGroup Label="CompilationProperties">
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<RootNamespace>$(ProjectName)</RootNamespace>
		<OlympusImplicitUsings>enable</OlympusImplicitUsings>
		<OlympusImplicitReferences>enable</OlympusImplicitReferences>
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		<SuppressNETCoreSdkPreviewMessage>true</SuppressNETCoreSdkPreviewMessage>
		<EnableConfigurationBindingGenerator>true</EnableConfigurationBindingGenerator>
		<IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
		<CompilerGeneratedFilesOutputPath>$(BaseIntermediateOutputPath)\Generated</CompilerGeneratedFilesOutputPath>
		<InterceptorsNamespaces>$(InterceptorsNamespaces);Microsoft.Extensions.Configuration.Binder.SourceGeneration</InterceptorsNamespaces>
	</PropertyGroup>

	<!-- Compilation Properties for Aspire Application Package -->
	<PropertyGroup Label="CompilationProperties" Condition="$(IsAspireApplicationPackage)">
		<IsAspireSharedProject>true</IsAspireSharedProject>
	</PropertyGroup>

	<!-- Compilation Properties for Aspire Application Host -->
	<PropertyGroup Label="CompilationProperties" Condition="$(IsAspireApplicationHost)">
		<OutputType>Exe</OutputType>
	</PropertyGroup>

</Project>