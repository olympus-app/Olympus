<Project>

	<!-- Imports -->
	<Import Project="..\..\..\Global.props" />

	<!-- Project -->
	<PropertyGroup>
		<InvariantGlobalization>true</InvariantGlobalization>
		<RunAOTCompilation>true</RunAOTCompilation>
		<IsAotCompatible>true</IsAotCompatible>
		<IsTrimmable>true</IsTrimmable>
		<IsPackable>true</IsPackable>
	</PropertyGroup>

	<!-- Packages -->
	<ItemGroup>
		<PackageReference Include="Riok.Mapperly" Version="4.2.1" />
	</ItemGroup>

	<!-- Dependencies -->
	<ItemGroup>
		<ProjectReference Include="$(SolutionFolder)\Libraries\Shared\Shared.csproj" />
	</ItemGroup>

	<!-- Usings -->
	<ItemGroup>
		<Using Include="Humanizer" />
		<Using Include="Olympus.Shared" />
		<Using Include="Olympus.Core.Archend" />
		<Using Include="Riok.Mapperly.Abstractions" />
	</ItemGroup>

</Project>