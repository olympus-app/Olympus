<Project Sdk="Microsoft.NET.Sdk.Razor">

	<!-- Project Properties -->
	<PropertyGroup Label="ProjectProperties">
		<ProjectType>ApplicationPackage</ProjectType>
		<ProjectLayer>Frontend</ProjectLayer>
		<ApplicationType>Web</ApplicationType>
	</PropertyGroup>

	<!-- Project References -->
	<ItemGroup Label="ProjectReferences">
		<ProjectReference Include="$(CoreFrontendProjectFile)" />
	</ItemGroup>

	<!-- Package References -->
	<ItemGroup Label="PackageReferences">
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" />
	</ItemGroup>

</Project>