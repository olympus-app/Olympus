<Project>

	<!-- Project Imports -->
	<ImportGroup Label="ProjectImports">
		<Import Project="$(CoreArchendProjectProps)" Condition="Exists('$(CoreArchendProjectProps)')" />
	</ImportGroup>

	<!-- Project Usings -->
	<ItemGroup Label="ProjectUsings" Condition="$(OlympusImplicitUsings)=='enable'">
		<Using Include="Olympus.Core.Backend" />
		<Using Include="Olympus.Core.Backend.Database" />
		<Using Include="Olympus.Core.Backend.Entities" />
		<Using Include="Olympus.Core.Backend.Identity" />
		<Using Include="Olympus.Core.Backend.Modules" />
		<Using Include="Olympus.Core.Backend.Storage" />
	</ItemGroup>

	<!-- Package Usings -->
	<ItemGroup Label="PackageUsings" Condition="$(OlympusImplicitUsings)=='enable'">
		<Using Include="EFCoreSecondLevelCacheInterceptor" />
		<Using Include="FastEndpoints" />
		<Using Include="FastEndpoints.Swagger" />
		<Using Include="FastEndpoints.Void" Alias="Void" />
		<Using Include="Gridify" />
		<Using Include="Gridify.EntityFramework" />
		<Using Include="Microsoft.AspNetCore.Http" />
		<Using Include="Microsoft.AspNetCore.Mvc" />
		<Using Include="Microsoft.EntityFrameworkCore" />
		<Using Include="Microsoft.EntityFrameworkCore.Metadata.Builders" />
		<Using Include="Riok.Mapperly.Abstractions" />
		<Using Include="System.Net" />
	</ItemGroup>

</Project>